<h6>Leaderboard</h6>
<div class="dashboard-table">
    <div class="dashboard-table-row">
      <div class="dashboard-table-cell header">Swapper</div>
      <div class="dashboard-table-cell header">Average Stars</div>
    </div>
  <% @group.leaderboard_users.each do |member| %>
    <div class="dashboard-table-row">
      <div class="dashboard-table-cell"><%= member.name %></div>
      <div class="dashboard-table-cell"><%= member.average_rating %></div>
    </div>
  <% end %>
</div>